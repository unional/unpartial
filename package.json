{
  "name": "unpartial",
  "version": "0.0.0-development",
  "description": "Unpartial a partialed object",
  "homepage": "https://github.com/unional/unpartial",
  "bugs": {
    "url": "https://github.com/unional/unpartial/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/unional/unpartial.git"
  },
  "license": "MIT",
  "author": {
    "name": "Homa Wong (unional)",
    "email": "homawong@gmail.com"
  },
  "sideEffects": false,
  "type": "module",
  "exports": {
    "import": "./esm/index.js",
    "require": "./cjs/index.js"
  },
  "main": "./cjs/index.js",
  "types": "./esm/index.d.ts",
  "files": [
    "cjs",
    "esm",
    "ts",
    "!ts/**/*.spec.ts"
  ],
  "scripts": {
    "build": "run-p build:cjs build:esm",
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "build:esm": "tsc -p tsconfig.esm.json",
    "clean": "rimraf coverage cjs esm lib libm && mkdir cjs && echo '{\"type\":\"commonjs\"}' > cjs/package.json || true",
    "coverage": "cross-env NODE_OPTIONS=--experimental-vm-modules jest --coverage",
    "depcheck": "depcheck",
    "lint": "eslint --ext=ts,js .",
    "postinstall": "husky install",
    "prepack": "pinst --disable",
    "postpack": "pinst --enable",
    "size": "size-limit",
    "test": "cross-env NODE_OPTIONS=--experimental-vm-modules jest",
    "verify": "npm-run-all clean -p build depcheck lint -p coverage size",
    "watch": "cross-env NODE_OPTIONS=--experimental-vm-modules jest --watch",
    "watch:type": "tsc --watch"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.0.1",
    "@commitlint/config-conventional": "^17.0.0",
    "@semantic-release/changelog": "^6.0.1",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^8.0.4",
    "@semantic-release/npm": "^9.0.1",
    "@size-limit/preset-small-lib": "^7.0.8",
    "@types/jest": "^29.0.0",
    "@typescript-eslint/eslint-plugin": "^5.26.0",
    "cross-env": "^7.0.3",
    "depcheck": "^1.4.3",
    "eslint": "^8.16.0",
    "eslint-plugin-harmony": "^7.1.0",
    "husky": "^8.0.1",
    "jest": "^29.0.0",
    "jest-validate": "^29.0.0",
    "jest-watch-suspend": "^1.1.2",
    "jest-watch-toggle-config": "^2.0.1",
    "jest-watch-typeahead": "^1.1.0",
    "npm-run-all": "^4.1.5",
    "pinst": "^3.0.0",
    "rimraf": "^3.0.2",
    "semantic-release": "^19.0.2",
    "size-limit": "^7.0.8",
    "ts-jest": "^28.0.8",
    "type-plus": "^4.11.1",
    "typescript": "^4.8.2"
  },
  "packageManager": "yarn@3.2.1",
  "engines": {
    "node": ">=6"
  },
  "size-limit": [
    {
      "path": "./cjs/index.js",
      "limit": "5 KB"
    },
    {
      "path": "./esm/index.js",
      "limit": "5 KB"
    }
  ]
}
