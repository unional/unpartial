{
  "name": "unpartial",
  "version": "1.0.4",
  "description": "Unpartial a partialed object",
  "homepage": "https://github.com/unional/unpartial",
  "bugs": {
    "url": "https://github.com/unional/unpartial/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/unional/unpartial.git"
  },
  "license": "MIT",
  "author": {
    "name": "Homa Wong (unional)",
    "email": "homawong@gmail.com"
  },
  "sideEffects": false,
  "type": "module",
  "exports": {
    "import": "./esm/index.js",
    "require": "./cjs/index.js",
    "default": "./esm/index.js"
  },
  "main": "./cjs/index.js",
  "types": "./esm/index.d.ts",
  "files": [
    "cjs",
    "esm",
    "ts",
    "!ts/**/*.spec.ts"
  ],
  "scripts": {
    "build": "run-p build:cjs build:esm",
    "build:cjs": "tsc -p tsconfig.cjs.json && ncp package.cjs.json cjs/package.json",
    "build:esm": "tsc -p tsconfig.esm.json",
    "clean": "rimraf coverage cjs esm lib libm || true",
    "coverage": "cross-env NODE_OPTIONS=--experimental-vm-modules jest --coverage",
    "cs": "changeset",
    "depcheck": "depcheck",
    "lint": "eslint --ext=ts,js .",
    "nuke": "run-s clean nuke:local",
    "nuke:local": "rimraf node_modules || true",
    "postinstall": "husky install",
    "prepack": "pinst --disable",
    "postpack": "pinst --enable",
    "release": "pnpm cs publish",
    "size": "size-limit",
    "test": "cross-env NODE_OPTIONS=--experimental-vm-modules jest",
    "verify": "npm-run-all clean -p build depcheck lint -p coverage size",
    "version": "pnpm cs version",
    "watch": "cross-env NODE_OPTIONS=--experimental-vm-modules jest --watch",
    "watch:type": "tsc --watch"
  },
  "devDependencies": {
    "@changesets/cli": "^2.24.4",
    "@commitlint/cli": "^17.1.2",
    "@commitlint/config-conventional": "^17.1.0",
    "@size-limit/preset-small-lib": "^8.0.1",
    "@types/jest": "^29.0.0",
    "@types/node": "^18.7.14",
    "@typescript-eslint/eslint-plugin": "^5.36.1",
    "@typescript-eslint/parser": "^5.36.1",
    "cross-env": "^7.0.3",
    "depcheck": "^1.4.3",
    "eslint": "^8.23.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-harmony": "^7.1.0",
    "husky": "^8.0.1",
    "jest": "^29.0.1",
    "jest-validate": "^29.0.1",
    "jest-watch-suspend": "^1.1.2",
    "jest-watch-toggle-config": "^2.0.1",
    "jest-watch-typeahead": "^2.1.1",
    "ncp": "^2.0.0",
    "npm-run-all": "^4.1.5",
    "pinst": "^3.0.0",
    "rimraf": "^3.0.2",
    "size-limit": "^8.0.1",
    "ts-jest": "^29.0.0",
    "type-plus": "^4.12.0",
    "typescript": "^4.8.3"
  },
  "packageManager": "pnpm@7.13.0",
  "engines": {
    "node": ">=6"
  },
  "size-limit": [
    {
      "path": "./cjs/index.js",
      "limit": "5 KB"
    },
    {
      "path": "./esm/index.js",
      "limit": "5 KB"
    }
  ]
}
