{
  "cSpell.words": [
    "codeql",
    "infile",
    "jsnext",
    "libm",
    "pinst",
    "snyk",
    "unpartial"
  ],
  "json.schemas": [
    {
      "fileMatch": [
        "tsconfig.*.json"
      ],
