{
  "[javascript]": {
    "editor.defaultFormatter": "vscode.typescript-language-features"
  },
  "[typescript]": {
    "editor.defaultFormatter": "vscode.typescript-language-features"
  },
  "cSpell.words": [
    "codeql",
    "infile",
    "jsnext",
    "libm",
    "pinst",
    "repobuddy",
    "snyk",
    "tsbuildinfo",
    "unpartial"
  ],
  "json.schemas": [
    {
      "fileMatch": [
        "tsconfig.*.json"
      ],
      "url": "http://json.schemastore.org/tsconfig"
    }
  ],
  "search.exclude": {
    "**/bower_components": true,
    "**/node_modules": true,
    "dist-es5": true
  },
  "typescript.tsdk": "node_modules/typescript/lib"
}